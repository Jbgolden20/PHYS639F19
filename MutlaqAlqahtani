import numpy as np
x1=0
x2=2
x=np.linspace(x1,x2,1001)
dx=(x2-x1)/(1000)
f=x

integral=0
for i in range(0,1000):
    integral+=(f[i]+f[i+1])/2*dx
print("the obtained result is ",integral)
# here the program takes the averege between two pionts x1 and x2 and divides it by 1000 to evalute the integral
# by finding the area of two adjacent pionts multiple times
